{
  "$schema": "https://dl.viam.dev/module.schema.json",
  "module_id": "edss:rocket-sensors",
  "visibility": "public",
  "url": "https://github.com/edss/rocket-sensors",
  "description": "Sensors used for home made rocket control system",
  "models": [
    {
      "api": "rdk:component:sensor",
      "model": "edss:rocket-sensors:mpu-sensor",
      "short_description": "MPU 6050 sensor for acceleration and gyroscope measurements",
      "markdown_link": "README.md#3-imu-sensor"
    },
    {
      "api": "rdk:component:sensor",
      "model": "edss:rocket-sensors:bmp-sensor",
      "short_description": "BMP sensor for pressure and altitude measurements",
      "markdown_link": "README.md#2-bmp-altitude-sensor"
    },
    {
      "api": "rdk:component:sensor",
      "model": "edss:rocket-sensors:loadcell",
      "short_description": "HX711 load cell sensor for weight measurements",
      "markdown_link": "README.md#1-hx711-loadcell-sensor"
    }
  ],
  "applications": null,
  "markdown_link": "README.md",
  "entrypoint": "dist/main",
  "first_run": "",
  "build": {
    "build": "./build.sh",
    "setup": "./setup.sh",
    "path": "dist/archive.tar.gz",
    "arch": [
      "linux/amd64",
      "linux/arm64"
    ]
  }
}